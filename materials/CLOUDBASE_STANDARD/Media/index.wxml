<wxs src="/common/utils.wxs" module="wxsUtils"/>
<a id="id1" style="{{id1.style}};{{style}}" className="wa-comp-CLOUDBASE_STANDARD-Media {{id1.className}} {{className}}" title="{{id1.title}}" bind:tap="onid1$tap">
	<a id="id10" style="{{id10.style}}" className="{{id10.className}}" title="{{id10.title}}">
		<c id="id3" style="{{id3.style}}" className="{{id3.className}}" wx:if="{{id3._waIf}}" alt="{{id3.alt}}" src="{{wxsUtils._getStaticResourceAttribute(mediaUrl)}}" mode="{{id3.mode}}" lazyLoad="{{id3.lazyLoad}}" showMenuByLongpress="{{id3.showMenuByLongpress}}"/>
		<slot name="customMedia" wx:if="{{id11._waIf}}"/>
	</a>
	<a id="id4" style="{{id4.style}}" className="{{id4.className}}" title="{{id4.title}}">
		<a id="id5" style="{{id5.style}}" className="{{id5.className}}" wx:if="{{id5._waIf}}" title="{{id5.title}}">
			<b id="id7" style="{{id7.style}}" className="{{id7.className}}" wx:if="{{title}}" text="{{title}}">{{title}}</b>
			<b id="id8" style="{{id8.style}}" className="{{id8.className}}" text="{{subtitle}}">{{subtitle}}</b>
		</a>
		<a id="id6" style="{{id6.style}}" className="{{id6.className}}" wx:if="{{content}}" title="{{id6.title}}">
			<b id="id9" style="{{id9.style}}" className="{{id9.className}}" text="{{content}}">{{content}}</b>
		</a>
		<slot name="customContent" wx:if="{{id12._waIf}}"/>
	</a>
</a>
