  .wa-comp-CLOUDBASE_STANDARD-Row.lcap-flex-col {
    flex-direction: column;
  }
  .wa-comp-CLOUDBASE_STANDARD-Row.lcap-flex-col-reverse {
    flex-direction: column-reverse;
  }
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-flex-row-reverse {
  flex-direction: row-reverse;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-justify-content-flex-start {
