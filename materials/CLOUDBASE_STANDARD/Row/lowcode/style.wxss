.wa-comp-CLOUDBASE_STANDARD-Row {
  /**
     * 使用less书写样式
     * */
}
.wa-comp-CLOUDBASE_STANDARD-Row:host {
  width: 100%;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-row {
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-flex-wrap-nowrap {
  flex-wrap: nowrap;
  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-flex-wrap-nowrap::-webkit-scrollbar {
  display: none;
}
