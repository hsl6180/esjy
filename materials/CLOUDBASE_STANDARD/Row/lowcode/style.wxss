}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-justify-content-center {
  justify-content: center;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-justify-content-flex-end {
  justify-content: flex-end;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-justify-content-space-around {
  justify-content: space-around;
}
.wa-comp-CLOUDBASE_STANDARD-Row.lcap-justify-content-space-between {
