.wa-comp-CLOUDBASE_STANDARD-TabBar {
  /**
     * 使用less书写样式
     * */
}
.wa-comp-CLOUDBASE_STANDARD-TabBar.lcap-tabbar {
  box-sizing: content-box;
  position: fixed;
  z-index: 1000;
  left: 0;
  right: 0;
  height: 96rpx;
  display: flex;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}
.wa-comp-CLOUDBASE_STANDARD-TabBar.lcap-tabbar.bottom {
  bottom: 0;
  bottom: -env(safe-area-inset-bottom);
}
.wa-comp-CLOUDBASE_STANDARD-TabBar.lcap-tabbar.top {
  top: 0;
}
.wa-comp-CLOUDBASE_STANDARD-TabBar.lcap-tabbar.unset {
  position: static;
}
