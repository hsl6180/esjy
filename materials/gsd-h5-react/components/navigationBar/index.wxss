.weda-menu-mobile {
  display: block;
  position: relative;
  z-index: 1000;
  width: 100%;
}

.weda-menu-mobile .weda-menu__wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20px 0 16px;
  height: 64px;
  background-color: #fff;
  position: relative;
  box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.05);
  box-sizing: border-box;
}

.weda-menu-mobile .weda-menu__bar-main {
  flex: 1;
  display: flex;
  align-items: center;
}

.weda-menu-mobile .weda-menu__bar-main .weda-menu__main-logo {
  font-weight: normal;
  font-size: 18px;
  color: #000000;
  text-decoration: none;
  display: flex;
  align-items: center;
}

.weda-menu-mobile .weda-menu__bar-main .weda-menu__main-icon {
  width: 32px;
  height: 32px;
  margin-right: 12px;
}

.weda-menu-mobile .weda-menu__bar-extra {
  display: flex;
  align-items: center;
  cursor: pointer;
}

.weda-menu-mobile .weda-menu__bar-extra .weda-menu__bar-btn,
.weda-menu-mobile .weda-menu__bar-extra .weda-menu__bar-btn--close {
  width: 24px;
  height: 24px;
  font-size: 24px;
}
.weda-menu-mobile .weda-menu__bar-extra .weda-menu__bar-btn--close {
  font-size: 28px;
}
.weda-menu-mobile__drawer {
  width: 100%;
  position: fixed;
  left: 0;
  right: auto;
  bottom: 0;
  top: 0;
  flex-direction: column;
  background-color: #fff;
  box-shadow: 0 0.14286rem 0.28571rem 0 rgb(54 58 80 / 32%);
  display: flex;
  color: rgba(0, 0, 0, 0.9);
  z-index: 1001;
}

.weda-menu-mobile__drawer .weda-drawer-drawer__body {
  padding: 0;
  flex: 1 1 auto;
  overflow: auto;
}

.weda-menu-mobile__drawer .weda-drawer-menu--light {
  position: relative;
  width: 100%;
  background-color: #1e222d;
  display: flex;
  flex-direction: column;
  height: 100%;
  vertical-align: top;
  transition: all 0.2s cubic-bezier(0.4, 0, 0.2, 1);
  overflow-x: hidden;
  background-color: #fff;
